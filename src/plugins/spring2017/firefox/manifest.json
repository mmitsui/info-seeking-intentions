{
    "update_url": "https://clients2.google.com/service/update2/crx",
    
    "manifest_version": 2,
    "name": "Workplace Study Extension",
    "description": "Companion extension for Workplace Study, 2017.",
    "version": "1.0",
    "content_scripts": [],
    "background": {
        "scripts": ["background.js", "external/jquery.js"]
    },
    "web_accessible_resources": ["*.png","img/*.png"],
    
    "homepage_url": "http://coagmento.org/EOPN/index.php",
    "permissions": [
                    "activeTab",
                    "history",
                    "storage",
                    "tabs",
                    "webNavigation",
                    "http://*/",
                    "https://*/"
                    ],
    
    "browser_action":{
        "browser_style": true,
        "default_popup": "popup/choose_page.html",
        "default_icon":{
            "48": "icons/logo-48.png",
            "32": "icons/logo-small.png",
            "16": "icons/logo-tiny.png"
            
        }
        
    },
    "icons": {
        "48": "icons/logo-48.png",
        "32": "icons/logo-small.png",
        "16": "icons/logo-tiny.png"
    },
    "offline_enabled": true,
    "minimum_chrome_version": "35"
}
